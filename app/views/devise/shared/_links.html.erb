<%- if controller_name != 'sessions' %>
  <%= link_to t('links.sign_in'), new_session_path(resource_name), class: 'hover:text-yellow-500' %><br>
<% end %>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t('links.register'), new_registration_path(resource_name), class: 'hover:text-yellow-500' %><br>
<% end %>
